---
export interface Props {
	title: string
}

const { title } = Astro.props
---

<!DOCTYPE html>
<html lang='en' class=''>
	<head>
		<meta charset='UTF-8' />
		<meta name='viewport' content='width=device-width' />
		<link rel='icon' type='image/svg+xml' href='/favicon.svg' />
		<meta name='generator' content={Astro.generator} />
		<link
			href='https://fonts.googleapis.com/css2?family=Blinker:wght@400;700&family=Nunito:wght@400;700&display=swap'
			rel='stylesheet'
		/>
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>

